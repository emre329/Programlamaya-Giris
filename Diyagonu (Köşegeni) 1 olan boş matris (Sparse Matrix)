import java.util.Scanner;

public class NewClass {
    public static void main(String[] args) {
        for(int i = 1 ; i<=10 ; i++){
            System.out.println("");
            for(int k = 1 ; k<=10 ; k++){
                if(i==k){
                    System.out.print("1");
                }else{
                    System.out.print("0");
                }
            }
        }
    }
